{
    "name": "atelierspierrot/demobuilder",
    "type": "application",
    "title": "DemoBuilder",
    "description": "PHP package to quickly construct, visualize and generate some demonstration pages in HTML5/CSS3",
    "keywords": [ "demonstration", "generator" ],
    "homepage": "http://github.com/AteliersPierrot/DemoBuilder",
    "version": "1.0.0",
    "license": "GPL-3.0",
    "authors": [{
            "name": "Pierre Cassat",
            "email": "me@e-piwi.fr",
            "homepage": "http://github.com/piwi"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "twig/twig": "1.*",
        "atelierspierrot/webfilesystem": "dev-master"
    },
    "autoload": {
        "psr-0": { 
            "Commons": "src",
            "FileSystem": "src",
            "DemoBuilder": "src",
            "DemoBuilder_": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/atelierspierrot/webfilesystem"
        }
    ],
    "config": {
        "vendor-dir": "src/vendor",
        "bin-dir": "bin"
    }
}
