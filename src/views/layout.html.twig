<!DOCTYPE html>
<html lang="{{ language }}">
<head>
    <meta charset="{{ charset }}">
    <title>{% block title %}{% endblock %} - {{ package.title }}</title>
    {% block metas %}{% endblock %}
    <link href="{{ vendor_assets }}bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{ assets }}styles.css" rel="stylesheet">
    <link href="{{ vendor_assets }}bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet">
    <!--[if lt IE 9]>
        <script src="{{ vendor_assets }}html5shiv/html5shiv.js"></script>
    <![endif]-->
    {% block stylesheets %}{% endblock %}
</head>
<body {% block body_attributes %}{% endblock %}>

    {% block body %}{% endblock %}

    <div id="message_box" class="msg_box"></div>

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="{{ vendor_assets }}jquery/jquery-1.9.1.min.js"><\/script>')</script>
<script>$.uiBackCompat = false;</script>
<script src="{{ vendor_assets }}bootstrap/js/bootstrap.min.js"></script>
<script src="{{ assets }}scripts.js"></script>
{% block scripts %}
<script>
$(function() {
    addCSSValidatorLink('perso/styles.css');
    addHTMLValidatorLink();
});
</script>
{% endblock %}
</body>
</html>
